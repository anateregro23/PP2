package Problema12;

/**
 *
 * @author ACER-HP
 */
public class Problema12 {
    /**
     * @param a es tipo integer arreglo.
     */
    public static void Probdoce (int a []) {
        int b = 0; 
        for (int c = 1; c <= 10; c = c + 2) { 
            b = b + a[c]; 
        } 
        double m = b / 5; 
        System.out.print("Media de numeros pares es: " + m); 
    }
}
