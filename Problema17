package Problema17;

/**
 *
 * @author ACER-HP
 */
public class Problema17 {
    /**
     * @param a es integer y tiene informacion primordial.
     * @param b es integer y tiene informacion primordial.
     */
    public static void C (int a, int b) {
        int c; 
        do { 
            c = a % b;
            a = b;
            b = c; 
        } while (c == 0); 
        System.out.println("Su MCD es: " + c);
    }
}
